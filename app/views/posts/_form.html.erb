<%= turbo_frame_tag "new_post" do %>
    <%= form_with model: post do |f| %>
        <% post.errors.full_messages.each do |message| %>
            <%= message %>
        <% end %>
        <%= f.text_field :body %>
        <%= f.file_field :image, direct_upload: true%>
        <%= f.submit "Post" %>
    <% end %>
<% end %>
